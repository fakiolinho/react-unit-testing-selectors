{
  "name": "react-quick-starter-kit",
  "version": "1.0.0",
  "description": "React starter kit without having to remove more than 50% of code to build a Single Page Application prototype",
  "main": "./dist/index.js",
  "author": {
    "name": "Fakiolas Marios",
    "email": "m.fakiolas@hotmail.gr",
    "url": "https://about.me/marios.fakiolas"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config ./webpack --color --progress",
    "predeploy": "yarn run build",
    "deploy": "cross-env NODE_ENV=production node dist/index.js",
    "precommit": "lint-staged",
    "prestart": "yarn",
    "start": "cross-env NODE_ENV=development webpack-dev-server --config ./webpack",
    "test": "cross-env NODE_ENV=test jest --config ./jest.config.json --no-cache",
    "eslint": "eslint client --color",
    "eslint:fix": "esw client --fix --watch --color",
    "stylelint": "stylelint -c .stylelintrc client/**/*.scss",
    "preanalyze": "cross-env NODE_ENV=production webpack --config webpack --colors --profile --json > stats.json && mv stats.json ./dist",
    "analyze": "webpack-bundle-analyzer ./dist/stats.json ./dist",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "travis": "yarn run build && yarn test && yarn run coveralls"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.{css,scss}": [
      "stylelint --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fakiolinho/react-quick-starter-kit.git"
  },
  "keywords": [
    "react",
    "redux",
    "redux-thunk",
    "jest",
    "enzyme",
    "express",
    "webpack",
    "babel"
  ],
  "license": "MIT",
  "dependencies": {
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "bem-cn": "^2.1.3",
    "normalize.css": "^6.0.0",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0-beta.2",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.4.1",
    "babel-loader": "^7.1.4",
    "babel-plugin-react-remove-properties": "^0.2.5",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.13",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-2": "^6.24.1",
    "coveralls": "^2.13.0",
    "cross-env": "^3.2.4",
    "css-loader": "^0.27.3",
    "cssnano": "^3.10.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.18.2",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-watch": "^3.1.3",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^0.10.1",
    "html-webpack-plugin": "^2.28.0",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.4.2",
    "lint-staged": "^7.0.0",
    "nock": "^9.0.11",
    "node-sass": "^4.7.2",
    "nodemon": "^1.11.0",
    "postcss-loader": "^1.3.3",
    "prettier": "^1.11.1",
    "react-addons-test-utils": "^15.6.2",
    "redux-mock-store": "^1.2.2",
    "redux-thunk": "^2.2.0",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.3",
    "stylelint": "^9.1.1",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-order": "^0.8.1",
    "stylelint-scss": "^2.5.0",
    "webpack": "^3.6.0",
    "webpack-bundle-analyzer": "^2.11.1",
    "webpack-dev-middleware": "^2.0.0",
    "webpack-dev-server": "^3.1.11",
    "webpack-hot-middleware": "^2.21.2",
    "webpack-merge": "^4.1.0"
  }
}
